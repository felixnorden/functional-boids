    "url": "https://github.com/felixnorden/functional-boids/issues"
  },
  "homepage": "https://github.com/felixnorden/functional-boids#readme",
  "devDependencies": {
    "nodemon": "^1.18.9"
  },
  "dependencies": {
    "psc-package": "^3.0.1",
    "pulp": "^12.3.0",
    "purescript": "^0.12.1"
  }
}
